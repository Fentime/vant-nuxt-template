{
  "name": "template",
  "version": "1.0.0",
  "description": "My ace Nuxt.js project",
  "author": "bmsherry",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "pm2:start": "cross-env NODE_ENV=production pm2 start server/index.js --name ssr",
    "pm2:loadBalance": "cross-env NODE_ENV=production pm2 start server/index.js -i max",
    "pm2:stop": "pm2 stop ssr",
    "pm2:delete": "pm2 delete ssr",
    "pm2:deleteAll": "pm2 delete all",
    "pm2:stopAll": "pm2 stop all"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.0.0",
    "vant": "^1.3.7"
  },
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "babel-eslint": "^8.2.1",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "express-session": "^1.15.6",
    "node-sass": "^4.9.4",
    "nodemon": "^1.11.0",
    "pm2": "^3.2.2",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1"
  }
}
